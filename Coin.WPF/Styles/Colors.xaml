<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <Color x:Key="BackGroundColor">#181A20</Color>
    <Color x:Key="TextColor">White</Color>
    <Color x:Key="BackGroundButtonColor">#2D313B</Color>
    <Color x:Key="NonActiveTextColor">#C7C8CA</Color>
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushBackGroundColor" Color="{StaticResource BackGroundColor}"/>
    <SolidColorBrush x:Key="BrushTextColor" Color="{StaticResource TextColor}"/>
    <SolidColorBrush x:Key="BrushBackGroundButtonColor" Color="{StaticResource BackGroundButtonColor}" />
    <SolidColorBrush x:Key="BrushNonActiveTextColor" Color="{StaticResource NonActiveTextColor}" />
</ResourceDictionary>